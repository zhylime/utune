{
  "colons": {
    "expect": "never",
    "error": true
  },
  "semicolons": {
    "expect": "never",
    "error": true
  },
  "commaSpace": {
    "expect": "always",
    "error": true
  },
  "depthLimit": 7,
  "indentPref": {
    "expect": 2,
    "error": false
  },
  "placeholder": "never",
  "namingConvention": "lowercase-dash",
  "namingConventionStrict": true,
  "extendPref": "@extend",
  "zIndexNormalize": 10
}