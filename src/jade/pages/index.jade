      .font-icon-btn-next.c-homepage__next-button.js-fullpage-next-button
    .section.c-homepage__2(data-js-switch-tab)
      .c-homepage__tab-1
      .c-homepage__tab-2
      .c-homepage__tab-3(data-js-skewing)
        img.lake(src=paths.img + "wedo-lake-light.png")
        img.hill(src=paths.img + "wedo-hill-last.png")
        img.hill(src=paths.img + "wedo-hill-mid.png")
        img.hill(src=paths.img + "wedo-hill-front.png")
        img.lake(src=paths.img + "wedo-lake.png")
        img.lake.lang(src=paths.img + "wedo-lake-ripple.png")
        img.lake.lang(src=paths.img + "wedo-lake-ripple2.png")
        img.lake.chuan(src=paths.img + "wedo-lake-fan.png")
        img.lake.chuan2(src=paths.img + "wedo-lake-fan2.png")
      .c-homepage__tab-4
        img.star.star1(src=paths.img + "star.svg")
        img.star.star2(src=paths.img + "star.svg")
        img.star.star3(src=paths.img + "star.svg")
        img.star.star4(src=paths.img + "star.svg")
        img.star.star5(src=paths.img + "star.svg")
        img.star.star6(src=paths.img + "star.svg")
        img.tree(src=paths.img + "wedo-tree.png")
      .c-homepage__tab-panel
        ul
          each item in weDoTabPanel
            li.font--roboto-light
              div.c-homepage__tab-panel__icon(class=item.icon)
              span!= item.text
              a.c-homepage__tab-panel__button(class=item.buttonClass href=item.url) View
      .font-icon-btn-next.c-homepage__next-button.js-fullpage-next-button
    .section.c-homepage__3
      .c-homepage__3--main
        each item in ourwork
          a.c-gallery__item.draw(href=item.href alt=item.alt class=item.widthClass+" "+item.floatClass)
            img.l-full-width(src=paths.img + item.img)
          if item.isEnd=="true"
            .clear
    .section.c-homepage__4
      h2 our clients
      //- (data-js-masonry)
      .c-masonry
        each item in clients
          a.c-masonry__item(href=item.href alt=item.alt)
            i
            img(src=paths.img + item.img)

      .font-icon-btn-next.c-homepage__next-button.js-fullpage-next-button

    .section.c-homepage__5
      h2 contact us
      .c-homepage__5__main
        .l-grid__col.l-col-10-5.l-pad--l-1.l-pad--r-1
          form
            .text-input.l-grid__col.l-col-10-5.l-pad--r-1
              input#name(type="text", name="your-name", required)
              label Your Name
              span
            .text-input.l-grid__col.l-col-10-5.l-pad--l-1
              input#email(type="text", name="your-email", required)
              label Your Email
              span
            .clear
            .text-input
              input#mySubject(type="text", name="your-subject", required)
              label Your Subject
              span
            .text-input
              textarea#myMessage(name="your-message", required)
              label Your Message
              span
            button.txt--uppercase.l-flt--r(type="submit") send your message
            .clear
        .l-grid__col.l-col-10-5
          .l-grid__col.l-col-10-5
            .font-icon-mail
            .c-homepage__5__email
              each item in contactEmail
                p!= item.name
                  br
                  a(href="mailto:#{item.email}")!= item.email
                br
                
          .l-grid__col.l-col-10-5.l-pad--l-1.l-pad--r-1
            div
              .font-icon-address
              .c-homepage__5__location
                p!= contactAddress.add
                br
                
                p!= contactAddress.postcode
                br
                
                p!= contactAddress.chineseAdd
            .clear
            div
              .font-icon-phone
              .c-homepage__5__phone
                p.
                  #{contactPhone.name}
                  <br>
                  #{contactPhone.number}
      .font-icon-btn-next.c-homepage__next-button.js-fullpage-next-button
    .section.c-homepage__6
      .c-homepage__6--logo
        img(src=paths.img + "logo-green.svg")
      .c-homepage__6--text.font--roboto-regular!=aboutUtune.text
      .c-homepage__6--footer.font--roboto-light!=aboutUtune.footer_text

