extends ../../_partials/_template
include ../../_mixins/_side-nav
include ../../_mixins/_legalFootNote

block metaKeywords
  meta(name='keywords', content="product planning,premium design,theme design,film implantation,industrial design,business gift,促品设计，专题设计，电影植入，产品工业设计，高端礼品")
block metaDescription
  meta(name="description", content="促品设计，专题设计，电影植入，产品工业设计，高端礼品，设计是一场与想象力的约会，拒绝套路，利用丰富的行业知识与产品经验，打造出与众不同优秀的落地产品。")
block content
  +side-nav(1)
  .c-chain.c-plan
    .c-chain__header.c-plan__header
      .c-plan__header-bg
        img(src=paths.img + "luowen.svg")
      .c-chain__header--title.font--bomb
        .c-chain__header--title-content Product planning
    .c-plan__main
      .c-plan__main-design
        .c-plan__main-design--content.c-plan__main-design--text.l-pad--r-3.l-pad--l-5.l-pad--t-2
          p 设计是一场与想象力的约会，拒绝套路，没有技巧，
            br
            | 善于赋予产品无形的技术，利用符号学、语义学的表达方式，
            br
            | 来创造优雅， 善于深入各种材料研究，
            br
            | 利用丰富的行业知识与产品经验，
            br
            | 打造出与众不同优秀的落地产品。
        .c-plan__main-design--content.l-pad--l-3.l-pad--r-5
          img.js-fancybox-thumb(src=paths.img + "plan-premium.png")
          a.fancybox.js-fancybox-img(href=paths.img + "premium/plan-premium-five.jpg" data-fancybox="images-premium" )
          a.fancybox(href=paths.img + "premium/plan-premium-IBM.jpg" data-fancybox="images-premium")
          a.fancybox(href=paths.img + "premium/plan-premium-MM.jpg" data-fancybox="images-premium")
          a.fancybox(href=paths.img + "premium/plan-premium-snickers.jpg" data-fancybox="images-premium")
          a.fancybox(href=paths.img + "premium/plan-premium-pocarisweat.jpg" data-fancybox="images-premium")
          a.fancybox(href=paths.img + "premium/plan-premium-pet-whiskas.jpg" data-fancybox="images-premium")
          a.fancybox(href=paths.img + "premium/plan-premium-pet-pedigree.jpg" data-fancybox="images-premium")
          a.fancybox(href=paths.img + "premium/plan-premium-dove-1.jpg" data-fancybox="images-premium")
          a.fancybox(href=paths.img + "premium/plan-premium-dove-2.jpg" data-fancybox="images-premium")
          a.fancybox(href=paths.img + "premium/plan-premium-yida.jpg" data-fancybox="images-premium")
          a.fancybox(href=paths.img + "premium/plan-premium-doublemint.jpg" data-fancybox="images-premium")
          a.fancybox(href=paths.img + "premium/plan-premium-lenovo-1.jpg" data-fancybox="images-premium")
          a.fancybox(href=paths.img + "premium/plan-premium-lenovo-2.jpg" data-fancybox="images-premium")
          a.fancybox(href=paths.img + "premium/plan-premium-cuixiangmi.jpg" data-fancybox="images-premium")
          a.fancybox(href=paths.img + "premium/plan-premium-caihongtang.jpg" data-fancybox="images-premium")

      .c-plan__main-design.l-pad--t-2
        .c-plan__main-design--content.l-pad--r-3.l-pad--l-5
          img.js-fancybox-thumb(src=paths.img + "plan-theme.png")
          a.fancybox.js-fancybox-img(href=paths.img + "theme/plan-theme-25year-1.jpg" data-fancybox="images-theme")
          a.fancybox(href=paths.img + "theme/plan-theme-25year-2.jpg" data-fancybox="images-theme")
          a.fancybox(href=paths.img + "theme/plan-theme-hallowmas.jpg" data-fancybox="images-theme")
          a.fancybox(href=paths.img + "theme/plan-theme-festival.jpg" data-fancybox="images-theme")
          a.fancybox(href=paths.img + "theme/plan-theme-christmas.jpg" data-fancybox="images-theme")
          a.fancybox(href=paths.img + "theme/plan-theme-srping-festival.jpg" data-fancybox="images-theme")
          a.fancybox(href=paths.img + "theme/plan-theme-sports-1.jpg" data-fancybox="images-theme")
          a.fancybox(href=paths.img + "theme/plan-theme-sports-2.jpg" data-fancybox="images-theme")
        .c-plan__main-design--content.l-pad--l-3.l-pad--r-0
          img(src=paths.img + "plan-01.png")
      .c-plan__main-design.l-pad--t-2
        .c-plan__main-design--content.l-pad--r-5.l-pad--l-0
          img(src=paths.img + "plan-02.png")
        .c-plan__main-design--content.l-pad--l-3.l-pad--r-3
          img.js-fancybox-thumb(src=paths.img + "plan-movie.png")
          a.fancybox.js-fancybox-img(href=paths.img + "film/plan-film-transformer.jpg" data-fancybox="images-film")
          a.fancybox(href=paths.img + "film/plan-film-monsterhunt.jpg" data-fancybox="images-film")
      .c-plan__main-design.l-pad--t-4
        .c-plan__main-design--content.l-pad--r-3.l-pad--l-5
          img.js-fancybox-thumb(src=paths.img + "plan-product.png")
          a.fancybox.js-fancybox-img(href=paths.img + "industrial/plan-industrial-milkcup.jpg" data-fancybox="images-industrial")
          a.fancybox(href=paths.img + "industrial/plan-industrial-catbowl.jpg" data-fancybox="images-industrial")
          a.fancybox(href=paths.img + "industrial/plan-industrial-doublemint.jpg" data-fancybox="images-industrial")
          a.fancybox(href=paths.img + "industrial/plan-industrial-lenovo.jpg" data-fancybox="images-industrial")
        .c-plan__main-design--content.l-pad--l-3.l-pad--r-0
          img(src=paths.img + "plan-03.png")
      .c-plan__main-design.l-pad--t-4.l-pad--b-4
        .c-plan__main-design--content.l-pad--r-5.l-pad--l-0
          img(src=paths.img + "plan-04.png")
        .c-plan__main-design--content.l-pad--l-3.l-pad--r-5
          img.js-fancybox-thumb(src=paths.img + "plan-gift.png")
          a.fancybox.js-fancybox-img(href=paths.img + "gift/plan-gift-ICBC.jpg" data-fancybox="images-gift")
          a.fancybox(href=paths.img + "gift/plan-gift-ThinkPad-20-1.jpg" data-fancybox="images-gift")
          a.fancybox(href=paths.img + "gift/plan-gift-ThinkPad-20-2.jpg" data-fancybox="images-gift")
          a.fancybox(href=paths.img + "gift/plan-gift-legend-capital.jpg" data-fancybox="images-gift")
          a.fancybox(href=paths.img + "gift/plan-gift-benz.jpg" data-fancybox="images-gift")
          a.fancybox(href=paths.img + "gift/plan-gift-bmw.jpg" data-fancybox="images-gift")
    .c-chain__footer.txt--white.c-plan__footer
      .c-chain__footer--title
        .c-chain__footer--line-before
        h2 WHY UTUNE
        .c-chain__footer--line-after
      h3 Team Vision Innovation Passion
         br
         | Standard Quality Profession System
      .c-chain__footer--icon
        ul
          each item,key in chain.utune
            li
              a(href="#")
                img(src=paths.img + item.img +"-plan.svg")
                span=item.txt

      +legalFootNote

